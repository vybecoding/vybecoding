{
  "name": "vybecoding",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "kill $(lsof -t -i:3000) 2>/dev/null || true && next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "test": "node test-runner.js",
    "convex": "convex dev",
    "demo": "kill $(lsof -t -i:8080) 2>/dev/null || true && python3 -m http.server 8080 --directory demo",
    "all": "npm run kill:all && sleep 2 && concurrently --kill-others \"npm run dev\" \"npm run demo\"",
    "kill:all": "lsof -ti:3000,3001,3002,8080 | xargs -r kill -9 2>/dev/null || true && sleep 1"
  },
